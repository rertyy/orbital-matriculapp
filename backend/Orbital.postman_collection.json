		{
			"name": "localhost:8080/register (Y)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const randomUser = randomString()\r",
							"const randomPassword = randomString()\r",
							"const randomEmail = randomString() + \"@\" + randomString() + \".\" + randomString(5)\r",
							"pm.globals.set(\"randomUser\", randomUser);\r",
							"pm.globals.set(\"randomPassword\", randomPassword);\r",
							"pm.globals.set(\"randomEmail\", randomUser);\r",
							"\r",
							"\r",
							"function randomString(maxLength = 30) {\r",
							"    const length = Math.floor(Math.random() * maxLength)\r",
							"    const results = []\r",
							"    const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r",
							"    for (i = 0; i < length; i++) {\r",
							"      const nextChar = characters.charAt(Math.floor(Math.random() * characters.length));\r",
							"      results.push(nextChar)\r",
							"    }\r",
							"    return results.join(\"\")\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\t\"username\": \"{{randomUser}}\",\r\n\t\"password\": \"password\",\r\n    \"email\": \"{{randomEmail}}\"\r\n}",
					"options": {
